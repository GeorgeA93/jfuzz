{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "checked": {
      "$id": "/properties/checked",
      "type": "boolean",
      "title": "The Checked Schema ",
      "default": false,
      "examples": [
        false
      ]
    },
    "dimensions": {
      "$id": "/properties/dimensions",
      "type": "object",
      "properties": {
        "width": {
          "$id": "/properties/dimensions/properties/width",
          "type": "integer",
          "title": "The Width Schema ",
          "default": 0
        },
        "height": {
          "$id": "/properties/dimensions/properties/height",
          "type": "integer",
          "title": "The Height Schema ",
          "default": 0,
          "examples": [
            10
          ]
        }
      },
      "required": [
        "width",
        "height"
      ]
    },
    "id": {
      "$id": "/properties/id",
      "type": "integer",
      "title": "The Id Schema ",
      "default": 0,
      "examples": [
        1
      ]
    },
    "date": {
      "$id": "/properties/date",
      "type": "string",
      "title": "The Date Schema ",
      "format": "date-time"
    },
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "title": "The Name Schema ",
      "maxLength": "10",
      "pattern": "a{3,6}d{20}"
    },
    "testing": {
      "$id": "/properties/testing",
      "type": "string",
      "title": "The testing Schema ",
      "minLength": "4",
      "maxLength": "25"
    },
    "thing": {
      "$id": "/properties/thing",
      "title": "The thing Schema ",
      "enum": ["thing_1", "thing_2", "thing_3"]
    },
    "price": {
      "$id": "/properties/price",
      "type": "number",
      "title": "The Price Schema ",
      "default": 0,
      "examples": [
        12.5
      ],
      "multipleOf": "0.01",
      "maximum": 1203,
      "minimum": 10
    },
    "tags": {
      "$id": "/properties/tags",
      "type": "array",
      "items": {
        "$id": "/properties/tags/items",
        "type": "string",
        "title": "The 0th Schema ",
        "default": "",
        "examples": [
          "home",
          "green"
        ]
      }
    }
  },
  "required": [
    "checked",
    "dimensions",
    "id",
    "name",
    "thing",
    "price",
    "tags"
  ]
}
